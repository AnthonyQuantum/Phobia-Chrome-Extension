{
    "name": "Phobia",
    "version": "0.1",
    "description": "Coming soon",
    "permissions": ["webNavigation", "*://*/*", "activeTab", "declarativeContent", "storage"],
    "background": {
      "scripts": ["config.js", "background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
      },
      "default_title": "Filters out all nasty content"
    },
    "icons": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
    },
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "css": ["blocker.css"],
        "js": ["config.js", "processImages.js"]
      }],
    "manifest_version": 2
  }